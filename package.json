{
  "name": "github-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite & graphql-codegen --watch",
    "build": "vue-tsc && vite build",
    "codegen": "graphql-codegen --watch",
    "preview": "vite preview",
    "format": "prettier . --write",
    "lint:code": "oxlint --deny-warnings --fix -c ./.eslintrc.json && eslint --ext .ts,.tsx src --fix",
    "lint:style": "stylelint 'src/**/*.{css,vue}' --fix",
    "lint": "yarn lint:code && yarn lint:style",
    "type": "vue-tsc --noEmit",
    "prepare": "husky"
  },
  "dependencies": {
    "@tanstack/vue-query": "^5.29.0",
    "@unovis/ts": "^1.4.0",
    "@unovis/vue": "^1.4.0",
    "@vueuse/core": "^10.9.0",
    "effector": "^23.2.0",
    "effector-vue": "^23.0.0",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "lucide-static": "^0.365.0",
    "primevue": "^3.51.0",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "vue-sonner": "^1.1.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.2.5",
    "@parcel/watcher": "^2.4.1",
    "@tanstack/eslint-plugin-query": "^5.28.11",
    "@types/node": "^20.12.5",
    "@typescript-eslint/parser": "^7.5.0",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-effector": "^0.14.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-oxlint": "^0.2.7",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "eslint-plugin-vue": "^9.24.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "oxlint": "^0.2.15",
    "postcss": "^8.4.38",
    "postcss-html": "^1.6.0",
    "prettier": "^3.2.5",
    "stylelint": "^16.3.1",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.4",
    "typescript-eslint": "^7.5.0",
    "vite": "^5.2.8",
    "vue-tsc": "^2.0.10"
  }
}
