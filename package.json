{
  "name": "github-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite & graphql-codegen --watch",
    "build": "vue-tsc && vite build",
    "codegen": "graphql-codegen --watch",
    "preview": "vite preview",
    "format": "prettier . --write",
    "lint:code": "eslint --ext .ts,.vue src --fix",
    "lint:style": "stylelint '**/*.{css,vue}' --fix",
    "lint": "yarn lint:code && yarn lint:style",
    "type": "vue-tsc --noEmit",
    "prepare": "husky"
  },
  "dependencies": {
    "@radix-icons/vue": "^1.0.0",
    "@tanstack/vue-query": "^5.24.1",
    "@vueuse/core": "^10.8.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "effector": "^23.2.0",
    "effector-vue": "^23.0.0",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "lucide-vue-next": "^0.340.0",
    "radix-vue": "^1.4.8",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "vue": "^3.4.19",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.2.4",
